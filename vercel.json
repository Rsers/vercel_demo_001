{
  "version": 2,
  "name": "vercel-demo-001",
  "outputDirectory": ".",
  "functions": {
    "api/edge-demo.js": {
      "runtime": "edge"
    }
  },
  "crons": [
    {
      "path": "/api/cron-cleanup",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin.html"
    }
  ],
  "redirects": [
    {
      "source": "/old-page",
      "destination": "/new-page",
      "permanent": true
    }
  ]
}